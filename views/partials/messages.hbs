{{# if errors}}
    {{# each errors}}
        <div class="message" role="alert">
            <h2 class="messages" >{{ this.msg }}</h2>    
        </div>
    {{/ each}}   
{{/ if}}

{{# if success_msg }}
    <div class="message" role="alert">
            <h2 class="messages" >{{ success_msg }}</h2>    
        </div>
{{/ if}}